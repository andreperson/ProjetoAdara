@{
    var img = "";
}

@if (ViewData["result"] != null)
{
    foreach (var menuitem in ViewData["result"] as List<Data.Entities.Menu>)
    {
        img = Domain.Util.config.UrlImg + "menu/ico-" + menuitem.descricao.ToLower() + ".png";
        <li class="nav-item">
            <a class="nav-link" href='@Url.Action(menuitem.view, menuitem.controller)/@menuitem.menuid'>
                <i class="@menuitem.icone"></i> @menuitem.descricao
            </a>
        </li>
    }
}




