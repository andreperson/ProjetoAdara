        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="">
                


                @{
                    bool home = false;
                    string actSolic = string.Empty;

                    string path = Request.Path;
                    int posic = path.IndexOf("/", 1);
                    if (posic > 0)
                    {
                        actSolic = path.Substring(1, posic - 1);
                    }

                    if (actSolic == "Home")
                    {
                        home = true;
                    }


                    if (!home)
                    {
                        <div id="submenu_tab"><span>@Html.Action("MenuSubHeader", "LayoutMenu")</span></div>
                    }

                }




                @*<h1><small>Sub Menu</small></h1>*@
                @*<ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> </a></li>
        <li class="active"></li>
      </ol>*@
            </section>

            <!-- Main content -->
